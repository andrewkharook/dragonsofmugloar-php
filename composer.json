{
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.1"
        }
    },
    "require": {
        "php": "7.1.*",
        "guzzlehttp/guzzle": "6.3",
        "monolog/monolog": "1.23",
        "roave/security-advisories": "dev-master",
        "symfony/options-resolver": "3.4"
    },
    "autoload": {
        "psr-4": {"DragonsOfMugloar\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"DragonsOfMugloar\\Tests\\": "tests/"}
    },
    "require-dev": {
        "phpunit/phpunit": "6.5"
    }
}
